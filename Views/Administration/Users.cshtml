<a asp-action="AssignRoles" class="btn btn-primary">@Localizer["Cancel"]</a>
<div class="col-12 p-3">
    <div class="card">
        <div class="card-header">
            <h3 class="h6 text-uppercase mb-0">@Localizer["All Users"]</h3>
        </div>
        <div class="card-body">
            <table id="DT_UsersLoad" class="table table-striped card-text" style="width:100%">
                <thead>
                    <tr>
                        <th>@Localizer["Name"]</th>
                        <th>@Localizer["Email"]</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

@section Scripts{
    <partial name="_DeleteAlertPartial" />
    <partial name="_UserListPartial" />
}

