@model IEnumerable<BugTracker.Models.Project>

<p>
    <a class="btn btn-primary" asp-action="Index">@Localizer["Go Back"]</a>
</p>
<h2>@Localizer["This deletes project, it's tickets with all attachments and comments, forever. Be absolutely sure what you are doing."]</h2>
<div class="col-12 border p-3">
    <table id="DT_ClosedProjectsLoad" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>@Localizer["Name"]</th>
                <th>@Localizer["Description"]</th>
                <th></th>

            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <partial name="_DeleteAlertPartial" />
    <partial name="_ClosedProjectListPartial" />
}